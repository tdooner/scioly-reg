  - if @team
    - admin = " as Admin" if !session[:user].nil?
    - admin ||= ""
    :javascript
      $(function() {
        mpq.register({"team": "#{@team.name}"}, "all", 31);
        mpq.name_tag("#{@team.name}#{admin}");
      });
  - if session[:user]
    :javascript
      $(function() {
        mpq.register({"admin": true}, "all", 31);
      }
