:javascript
  $(function() {
    $("table#admin-score-table").tablesorter({sortList: [[2,0]]});
  });

%table.zebra-striped#admin-score-table
  %thead
    %th Event Name
    %th Division 
    %th Scores Reported?
  %tbody
    - @events.each do |e|
      %tr
        %td=link_to(e.event, schedule_scores_url(e))
        %td=e.division
        %td 
          - if e.scores.length == 0 || e.scores_withheld
            =image_tag "error.png"
            - if e.scores_withheld
              (Withheld)
          - else 
            =image_tag "ok.png"
